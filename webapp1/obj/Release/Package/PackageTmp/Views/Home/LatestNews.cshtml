@{
    ViewBag.Title = "LatestNews";
}
<main aria-labelledby="title">
   



    <div class="resizable-container">
        <br />
        
        <h2>Bringing you the latest comics</h2>
    </div>

    <div class="container3">
        <div class="wgh-slider">
            <input class="wgh-slider-target" type="radio" id="slide-1" name="slider" />
            <input class="wgh-slider-target" type="radio" id="slide-2" name="slider" />
            <input class="wgh-slider-target" type="radio" id="slide-3" name="slider" checked="checked" />
            <input class="wgh-slider-target" type="radio" id="slide-4" name="slider" />
            <input class="wgh-slider-target" type="radio" id="slide-5" name="slider" />
            <div class="wgh-slider__viewport">
                <div class="wgh-slider__viewbox">
                    <div class="wgh-slider__container">
                        <div class="wgh-slider-item">
                            <div class="wgh-slider-item__inner">
                                <figure class="wgh-slider-item-figure">
                                    <img class="wgh-slider-item-figure__image" src="~/Content/latest5.jpg" alt="Photon" />
                                    <figcaption class="wgh-slider-item-figure__caption"><a href="https://www.marvel.com/comics/issue/101546/monica_rambeau_photon_2022_5">Photon</a><span></span></figcaption>
                                </figure>
                                <label class="wgh-slider-item__trigger" for="slide-1" title="Show Photon"></label>
                            </div>
                        </div>
                        <div class="wgh-slider-item">
                            <div class="wgh-slider-item__inner">
                                <figure class="wgh-slider-item-figure">
                                    <img class="wgh-slider-item-figure__image" src="~/Content/latest3.jpg" alt="The New Mutants" />
                                    <figcaption class="wgh-slider-item-figure__caption"><a href="https://www.marvel.com/comics/issue/107386/new_mutants_lethal_legion_2023_2">The New Mutants</a><span></span></figcaption>
                                </figure>
                                <label class="wgh-slider-item__trigger" for="slide-2" title="Show The New Mutants"></label>
                            </div>
                        </div>
                        <div class="wgh-slider-item">
                            <div class="wgh-slider-item__inner">
                                <figure class="wgh-slider-item-figure">
                                    <img class="wgh-slider-item-figure__image" src="~/Content/lastest2.jpg" alt="She-Hulk" />
                                    <figcaption class="wgh-slider-item-figure__caption"><a href="https://www.marvel.com/comics/issue/107442/she-hulk_2022_12">She-Hulk</a><span></span></figcaption>
                                </figure>
                                <label class="wgh-slider-item__trigger" for="slide-3" title="Show She-Hulk"></label>
                            </div>
                        </div>
                        <div class="wgh-slider-item">
                            <div class="wgh-slider-item__inner">
                                <figure class="wgh-slider-item-figure">
                                    <img class="wgh-slider-item-figure__image" src="~/Content/Latest1.jpg" alt="Investing in each other" />
                                    <figcaption class="wgh-slider-item-figure__caption"><a href="https://www.marvel.com/comics/issue/111549/investing_in_each_other_2023_1">Investing in each other</a><span>Quantic</span></figcaption>
                                </figure>
                                <label class="wgh-slider-item__trigger" for="slide-4" title="Show Investing In Each Other"></label>
                            </div>
                        </div>
                        <div class="wgh-slider-item">
                            <div class="wgh-slider-item__inner">
                                <figure class="wgh-slider-item-figure">
                                    <img class="wgh-slider-item-figure__image" src="~/Content/latest4.jpg" alt="Spidey,Amazing Friends" />
                                    <figcaption class="wgh-slider-item-figure__caption"><a href="https://www.marvel.com/comics/issue/107207/free_comic_book_day_2023_spidey_friends_2023_1~">Spidey Series</a><span></span></figcaption>
                                </figure>
                                <label class="wgh-slider-item__trigger" for="slide-5" title="Show Spidey And His Amazing Friends"></label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <br />

    <br />
    

</main>